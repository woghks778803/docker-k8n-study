{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "nginx",
            "image": "woghks778803/docker-nginx",
            "hostname": "nginx",
            "essential": true,
            "memory": 128,
            "links": ["frontend", "backend"],
            "portMappings": [
                {
                    "hostPort": "80",
                    "containerPort": "80"
                }
            ]
        },
        {
            "name": "frontend",
            "image": "woghks778803/docker-frontend",
            "hostname": "frontend",
            "essential": false,
            "memory": 128
        },
        {
            "name": "backend",
            "image": "woghks778803/docker-backend",
            "hostname": "backend",
            "essential": false,
            "memory": 128
        }
    ]
}